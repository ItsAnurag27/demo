# AWS
AWS_REGION=ap-south-1

# Cognito
COGNITO_CLIENT_ID=your_cognito_app_client_id
# Optional: only needed if your App Client has a client secret enabled
COGNITO_CLIENT_SECRET=
COGNITO_USER_POOL_ID=your_user_pool_id

# Server
PORT=3000
SESSION_SECRET=replace-with-a-long-random-string
COOKIE_SECURE=false

# Optional: auto-provision users on first OTP request (requires admin permissions)
COGNITO_AUTO_PROVISION=false
